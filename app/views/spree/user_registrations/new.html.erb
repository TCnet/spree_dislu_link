<% @body_id = 'signup' %>

  <div class="main-content main-content-login">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="breadcrumb-trail breadcrumbs">
            <ul class="trail-items breadcrumb">
              <li class="trail-item trail-begin">
                 <%= link_to Spree.t(:home), spree.root_path %>
              </li>
              <li class="trail-item trail-end active">
                <%=Spree.t(:signup)%>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <%= render :partial => 'spree/shared/dislu_flash_message' %>
      <%= render :partial => 'spree/shared/error_messages', :locals => { :target => @user } %>
      <div class="row">
        <div class="content-area col-lg-12 col-md-12 col-sm-12 col-xs-12">
          <div class="site-main">
            <h3 class="custom_blog_title">
              <%=Spree.t(:signup)%>
            </h3>
            <div class="customer_login">
              <div class="row">
               
                <div class="col-lg-6 col-md-6 col-sm-12 offset-lg-3 offset-md-3">
                  <div class="login-item">
                    <h5 class="title-login">Register now!</h5>
                     <%= render :partial => 'spree/shared/dislu_register' %>
                  </div>
                </div>
                 <div class="col-lg-6 col-md-6 col-sm-12">
                 

                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>